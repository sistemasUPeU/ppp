	<queryString>
		<![CDATA[SELECT   Esc.Nombre AS "ESCUELA", Fal.Nombre AS "FACULTAD", Tr.Tipo ||''|| U.Nombre ||' '|| U.Apellidos AS "DIRECTOR"
FROM  PPP_ALUMNO AL , Ppp_Escuela ESC  , 
Ppp_Facultad FAL , Ppp_Universidad UNI ,
Ppp_Escuela_Trabajador ET, Ppp_Periodo P , Ppp_Trabajador TR ,Ppp_Usuario U 
WHERE Al.Idescuela = ESC.Idescuela 
AND Esc.Idfacultad = Fal.Idfacultad AND Fal.Iduniversidad =  Uni.Iduniversidad
and   Esc.Idescuela = Et.Idescuela and  P.Idperiodo = Et.Idperiodo and  Et.Idtrabajador = Tr.Idtrabajador 
and  Tr.Idtrabajador = U.Idusuario
and Al.Idalumno = $P{txtId}]]>
	</queryString>
